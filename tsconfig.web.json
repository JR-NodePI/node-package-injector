{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "./global.d.ts" ,
    "./src/renderer/src/env.d.ts",
    "./src/renderer/src/**/*",
    "./src/renderer/src/**/*.tsx",
   ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}
